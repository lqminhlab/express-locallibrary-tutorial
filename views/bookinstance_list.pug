extends layout

block content
  h1= title

  ul
    - bookinstance_list.sort(function(a, b) {let textA = a.book.title.toUpperCase(); let textB = b.book.title.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;});
    each instance in bookinstance_list
      li
        a(href=instance.url) #{instance.book.title} 
        |  - #{instance .imprint}
        |  (#{instance.status})

    else
      li There are no bookinstance.