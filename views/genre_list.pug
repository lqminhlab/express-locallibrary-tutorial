extends layout

block content
  h1= title

  ul
    - categories.sort(function(a, b) {let textA = a.name.toUpperCase(); let textB = b.name.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;});
    each instance in categories
      li
        a(href=instance.url) #{instance.name}

    else
      li There are no bookinstance.